  "project": {
    "id": "171d1b69-99be-4425-bad1-24ae219613db",
    "title": "Monitoring large Mammal Communities in Forests surrounded by Urban and Agricultiural Matrix",
    "description": "This project aims to compare the composition of large mammals in peri-urban landscapes by examining forest patches surrounded by urban and agricultural areas. Specifically, we will investigate how the surrounding landscape influences the presence and absence of large mammal species. Sampling will be conducted using a random point approach, with consideration given to the species-area relationship. Larger forest patches will be sampled more intensively to account for potential differences in species richness.",
    "samplingDesign": "simpleRandom",
    "captureMethod": [
      "activityDetection"
    ],
    "individualAnimals": false,
    "observationLevel": [
      "event"
    ]
  },
